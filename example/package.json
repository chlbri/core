{
  "name": "@bemedev/core-examples",
  "version": "0.0.1",
  "description": "Examples and demonstrations for @bemedev/core library",
  "private": true,
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "config": "pnpm install",
    "config:off": "pnpm run config --offline",
    "config:lock": "pnpm run config --frozen-lockfile",
    "rm:cli": "pnpm run rimraf -- src/.bemedev .bemedev.json",
    "test:coverage": "vitest run",
    "rimraf": "pnpm dlx rimraf -rf",
    "rm": "pnpm run rimraf -- node_modules pnpm-lock.yaml",
    "pretest": "pnpm run config && pnpm run rm:cli",
    "test": "pnpm run test:coverage --no-coverage",
    "upgrade": "pnpm upgrade --latest"
  },
  "keywords": [
    "examples",
    "arithmetic",
    "math",
    "typescript"
  ],
  "author": {
    "email": "bri_lvi@icloud.com",
    "name": "chlbri",
    "url": "https://bemedev.vercel.app"
  },
  "license": "MIT",
  "devDependencies": {
    "@bemedev/vitest-alias": "^0.0.3",
    "@bemedev/vitest-exclude": "^0.1.1",
    "@bemedev/vitest-extended": "^1.5.3",
    "@types/shelljs": "^0.10.0",
    "core": "file:..",
    "shelljs": "^0.10.0",
    "ts-morph": "^27.0.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
