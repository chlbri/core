{
  "name": "@bemedev/core-examples",
  "version": "0.0.1",
  "description": "Examples and demonstrations for @bemedev/core library",
  "private": true,
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "codebase": "tsx src/cli.ts",
    "config": "pnpm install",
    "config:off": "pnpm run config --offline",
    "config:lock": "pnpm run config --frozen-lockfile",
    "cli:func": "pnpm run config && tsx src/cli.ts",
    "cli": "pnpm run cli:func && pnpm run cli:deep",
    "cli:deep": "pnpm core init && pnpm core add \"features.arrays.castings.freeze\" && pnpm core remove \"features.arrays.castings.tuple\"",
    "upgrade": "pnpm upgrade --latest"
  },
  "keywords": [
    "examples",
    "arithmetic",
    "math",
    "typescript"
  ],
  "author": {
    "email": "bri_lvi@icloud.com",
    "name": "chlbri",
    "url": "https://bemedev.vercel.app"
  },
  "license": "MIT",
  "devDependencies": {
    "@bemedev/vitest-extended": "^1.3.6",
    "core": "file:..",
    "ts-morph": "^26.0.0",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
